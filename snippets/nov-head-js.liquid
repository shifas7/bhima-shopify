      quickview: {{ 'products.product.quickview' | t | json }}
    },
    moneyFormat: {{ shop.money_format | json }},
    moneyFormatnojson: {{ shop.money_format | json }},
    freeshipping_value: {{ settings.free_shipping }},
    show_free_shipping: {{ settings.freeshipping_enable }},
    cart_note: {{ settings.cart_note }},
    cart_shipping_calculator: {{ settings.shipping_calculator }},
    cart_discount_code: {{ settings.discount_code }},
    proceed_to_checkout: {{ settings.terms_conditions | json }},
    cart_status: '{{ settings.cart_status_addtocart }}',
    terms_conditions_enable: {{ settings.terms_conditions_enable }},
  }
  window.variantStrings = {
    addToCart: `{{ 'products.product.add_to_cart' | t }}`,
    addTobag: `{{ 'products.product.add_to_bag' | t }}`,
    soldOut: `{{ 'products.product.sold_out' | t }}`,
    unavailable: `{{ 'products.product.unavailable' | t }}`,
    notify_me: `{{ 'products.product.notify_me' | t }}`,
    preorder: `{{ 'products.product.pre_order' | t }}`,
  }
  window.inventory_text = {
    add_to_cart: "{{ 'products.product.add_to_cart' | t }}",
    sold_out: "{{ 'products.product.sold_out' | t }}",
    preorder: "{{ 'products.product.pre_order' | t }}",
  }
  document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
  window.router = "";
  window.use_color_swatch = {{ settings.use_color_swatch }};
  window.option_color_swatch = '{{ settings.option_color_swatch | downcase }}';
  var vinovathemes = {
    main_info: {
      shop: '{{ shop.permanent_domain }}',
      domain: '{{ shop.domain }}',
      lic: '{{ settings.license | strip_newlines | remove: "'" | remove: "\" | remove: "/" }}',
      status: '{{ settings.purchase_action }}',
    }
  }
</script> 
